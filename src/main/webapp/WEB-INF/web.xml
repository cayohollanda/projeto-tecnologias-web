<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>projeto</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <page-encoding>UTF-8</page-encoding>
    </jsp-property-group>
  </jsp-config>
  <filter>
    <display-name>VerificaLoginFilter</display-name>
    <filter-name>VerificaLoginFilter</filter-name>
    <filter-class>br.com.iesp.projetoweb.filter.VerificaLoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>VerificaLoginFilter</filter-name>
    <url-pattern>/VerificaLoginFilter</url-pattern>
  </filter-mapping>
</web-app>